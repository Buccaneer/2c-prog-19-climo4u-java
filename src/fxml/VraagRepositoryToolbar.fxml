<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<fx:root type="HBox" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Button fx:id="btnSubvragen" mnemonicParsing="false" onAction="#subvragen" text="Subvragen over klimatogram">
         <HBox.margin>
            <Insets right="16.0" />
         </HBox.margin>
      </Button>
      <Button fx:id="btnDetermineren" mnemonicParsing="false" onAction="#determineren" text="Determineren van klimatogram">
         <HBox.margin>
            <Insets right="16.0" />
         </HBox.margin>
      </Button>
      <Button fx:id="btnLocaties" mnemonicParsing="false" onAction="#locatie" text="Locatie vragen">
         <HBox.margin>
            <Insets right="16.0" />
         </HBox.margin>
      </Button>
   </children>
   <padding>
      <Insets bottom="15.0" />
   </padding>
</fx:root>
